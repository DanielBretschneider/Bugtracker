<?xml version="1.0" encoding="utf-8" ?>
<configuration>
		<!--
	PROBLEM DESCRIPTION: Descriptions for problem categories.
	-->
	
	<problem-categories>
		<problem-category name="Allgemeiner Fehler." ticket= "af">
			<description text="Folgender Allgemeiner Fehler:"/>
			<app-selection>All</app-selection>
		</problem-category>
		<problem-category name="Bilder Bennenung" ticket="bbf">
			<description text="Die Bilder Bennenung ist falsch da:"/>
			<app-selection>Screen</app-selection>
		</problem-category>
		<problem-category name="IP8-Hanging" ticket="ip8-hanging">
			<description text="Im Hanging Protokoll stimm folgendens nicht:"/>
			<app-selection>IP8,Screen</app-selection>
		</problem-category>
		<problem-category name="XR" ticket="xr5-slow">
			<description text="Ist langsam oder reagiert nicht:"/>
			<app-selection>XR5</app-selection>
		</problem-category>
		<problem-category name="Word" ticket="Word">
			<description text="Macht folgendes Problem:"/>
		</problem-category>
		<problem-category name="TH"  ticket="th-Slow">
			<description text="Ist langsam oder reagiert nicht:"/>
		</problem-category>
		<problem-category name="Syngo Via" ticket="SyngoVia">
			<description text="Folgende Bilder fehlen:"/>
		</problem-category>
	</problem-categories>
	
	<!--
	TARGET: Targets where bugtracks will be sent
	default value -> \\10.74.10.100\bugTracker
	-->
  
  <targets>
		<target type="folder"  name="bugtracker network folder" path='\\10.74.10.100\bugTracker' foldername="Bugtracker-%computername%-%date%-%time%" default="true" > 
		</target>
	
		<target type="folder" name="bugtracker-test-folder-network" path='\\10.74.10.100\bugTracker\bugtrackerv2_test_target' default='false' foldername="Bugtracker-%computername%-%date%-%time%" >
		</target>
  </targets>
  
	<!--
	APPLICATION SECTION
	-->
	<applications>
		<!--
		<application name="SQL" standard="false" show="false">
			<query name ="SQLString" queryString="select 1"/>
		</application>
		-->
	
    <!--
		Bugtracker
		-->
    <application name="Bugtracker" executable ="C:\Bugtracker\" standard="true" show="always">
		<log location="client"
			 path="C:\Bugtracker\"
			 filename="*.log"
			 find="NEW"
			 />
		<!--pre-fetch path="C:\Users\Dominik Beyerle\Desktop\create_folder.ps1"/>
		<post-fetch path="C:\Users\Dominik Beyerle\Desktop\create_folder.ps1"/ -->
    </application>
	

    <!--
		ImagePro 7
		-->
		<application name="IP7" executable ="c:\Program Files (x86)\DATA\xr pacs\imagepro\" standard="true" show="installed">
			<log location="client"
				 path="c:\Program Files (x86)\DATA\xr pacs\imagepro\"
				 filename="xrpimagepro*.log"
				 find="NEW"/>
		
			<log location="host"
				 path="\\tsclient\c\Program Files (x86)\DATA\xr pacs\imagepro\"
				 filename="xrpimagepro*.log"
				 find="NEW"/>
		</application>
		<!--
		ImagePro 8
		-->
		<application name="IP8" executable ="C:\Program Files\DATA\xr pacs\ImagePro 8\" standard="true" show="installed">
			<log location="client"
				 path="C:\ProgramData\DATA\ImagePro8Logs"
				 filename="ImagePro8_*.log"
				 find="NEW"/>
			<log location="client"
				 path="\\tsclient\c\ProgramData\DATA\ImagePro8Logs"
				 filename="ImagePro8_*.log"
				 find="NEW"/>
		</application>

		<!--
		XR5 Manage
		-->
		<application name="XR5" executable="C:\Program Files (x86)\DATA\XR5\" standard="true" show="installed">
			<log location="server"
				 path="\\server\xr50\log\"
				 filename="xr5.*%clientname%*.log"
				 find="NEW"/>
			<log location="client"
				 path="%ProgramFiles%/data/"
				 filename="xrwordaddin.*.log"
				 find="NEW"/>
			
		</application>

		<!--
		timer Manage
		-->
		<application name="XR5 Timer" executable="C:\Program Files (x86)\DATA\XRTimer5" standard="true" show="installed">
			<log location="server"
				 path="\\server\xr50\log\"
				 filename="xr ris timer*%clientname%*.log"
				 find="NEW"/>
			<log location="client"
				 path="%ProgramFiles%/data/"
				 filename="xrristimer.*.log"
				 find="NEW"/>
		</application>

		<!--
		XR Wordaddin
		-->
		<application name="XR-Wordplugin" executable ="c:/program" standard="true" show="installed">
			<log location="server"
				 path="\\server\xr50\log\"
				 filename="xrwordaddin.*%clientname%*.log"
				 find="NEW"/>
				 
			<log location="client"
				 path="%ProgramFiles%/data/"
				 filename="xrwordaddin.*.log"
				 find="NEW"/>
		</application>

		<!--
		XR5 Importer
		-->
		<application name="Importer" executable ="c:/program" type="log" standard="false" show="always">
			<log location="server"
				 path="\\server\xr50\log\xrpimp"
				 filename="xrpimp_*%clientname%*.log"
				 find="NEW"/>
		</application>


		<!--
		changeviewer Manage
		-->
		<application name="Changer Viewer" executable="C:\viewerChanger" standard="false" show="installed">
			<log location="client"
				 path="c:\viewerChanger\"
				 filename="ViewChanger.log"
				 find="NEW"/>
			<log location="host"
				 path="\\tsclient\c\viewerChanger\"
				 filename="ViewChanger.log"
				 find="NEW"/>
		</application>

		<!--
		Speechmagic
		-->
		<application name="Speechmagic" executable ="c:/program" type="log" standard="true" show="installed">
			<log location="host"
				 path="\\tsclient\c\SpeechMagic\InterActive\Logging\"
				 filename ="*.log"
				 find="NEW"/> 
			<log location="client"
				 path="%ProgramData%\SpeechMagic\InterActive\Logging\"
				 filename ="*.log"
				 find="NEW"/> 
			<log location="host" 
				 path="\\tsclient\C\Users\%username%\AppData\Local\Philips Device Control Center\log\" 
				 filename ="*.log"
				 find="NEW"/> 
			<log location="client" 
				 path="%LOCALAPPDATA%\Philips Device Control Center\log\" 
				 filename ="*.log"
				 find="NEW" 
				 /> 
		</application>
		
	</applications>
	
</configuration>